class Superhero:
    def __init__(self, name, real_name, power, origin, team_affiliation):
        self.name = name
        self.real_name = real_name
        self.power = power
        self.origin = origin
        self.team_affiliation = team_affiliation
    
    def get_name(self):
        return self.name
    
    def get_real_name(self):
        return self.real_name
    
    def get_power(self):
        return self.power
    
    def get_origin(self):
        return self.origin
    
    def get_team_affiliation(self):
        return self.team_affiliation
    
    def get_full_info(self):
        return {
            'Name': self.name,
            'Real Name': self.real_name,
            'Power': self.power,
            'Origin': self.origin,
            'Team Affiliation': self.team_affiliation
        }


superman = Superhero("Superman", "Clark Kent", "Super strength, flight, invulnerability", "Krypton", "Justice League")
batman = Superhero("Batman", "Bruce Wayne", "Peak human condition, detective skills, martial arts", "Gotham City", "Justice League")

print(superman.get_name())  # Output: Superman
print(batman.get_real_name())  # Output: Bruce Wayne
print(superman.get_full_info())  

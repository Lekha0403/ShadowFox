class Avenger:
    def __init__(self, name, age, gender, super_power, weapon):
        self.name = name
        self.age = age
        self.gender = gender
        self.super_power = super_power
        self.weapon = weapon

    def __repr__(self):
        return (f"Avenger(Name: {self.name}, Age: {self.age}, Gender: {self.gender}, "
                f"Super Power: {self.super_power}, Weapon: {self.weapon})")

# Creating instances for each Avenger
captain_america = Avenger(name="Captain America", age=105, gender="Male", super_power="Super strength", weapon="Shield")
iron_man = Avenger(name="Iron Man", age=53, gender="Male", super_power="Technology", weapon="Armor")
black_widow = Avenger(name="Black Widow", age=34, gender="Female", super_power="Superhuman", weapon="Batons")
hulk = Avenger(name="Hulk", age=49, gender="Male", super_power="Unlimited Strength", weapon="No Weapon")
thor = Avenger(name="Thor", age=1500, gender="Male", super_power="Super Energy", weapon="Mjolnir")
hawkeye = Avenger(name="Hawkeye", age=41, gender="Male", super_power="Fighting skills", weapon="Bow and Arrows")

# Printing each Avenger instance
print(captain_america)
print(iron_man)
print(black_widow)
print(hulk)
print(thor)
print(hawkeye)
